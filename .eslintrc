{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "react",
    "prettier"
  ],

  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true
    }
  },
    "globals": {
    "fetch": false,
    "__DEV__": true
  },
  "rules": {
    "react/jsx-filename-extension": 0,
  }
}
