    "mobx-state-tree": "^1.1.0",
    "moment": "^2.12.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0-beta.5",
    "react-native": "^0.49.3",
    "react-native-i18n": "^2.0.9",
    "react-native-router-flux": "^4.0.0-beta.22",
    "react-native-vector-icons": "^4.4.2",
