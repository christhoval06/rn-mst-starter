{
  "name": "ReactNativeBoilerplate",
  "author": "Thomas LECLERCQ <contact@thomas.sh>",
  "description": "React Native App Starter Kit for Android and iOS",
  "license": "MIT",
  "bugs": "https://github.com/lecler-i/rn-mst-boilerplate",
  "repository": {
    "type": "git",
